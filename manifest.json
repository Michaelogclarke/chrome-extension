
{
  "manifest_version": 3,
  "name": "Keybind Testing",
  "description": "Jump to bookmarks",
  "version": "1.0",
  "action": {
     "default_popup": "/popup/popup.html"

  },
  "commands": {
    "_execute_action": {
        "suggested_key": {
            "default": "Alt+K"
            }
        }
    },
  "content_scripts": [
    {
        "matches": ["cleactadh"]

    }
    ]
  "host_permissions": [
    "https://*.example.com/"
  ],
  "permissions": [
    "storage",
    "bookmarks"
  ],
 "background": {
    "service_worker": "src/background/service-worker.js"
    }
}

